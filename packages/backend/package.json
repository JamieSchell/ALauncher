{
  "name": "@modern-launcher/backend",
  "version": "1.0.0",
  "description": "Backend server for Modern Minecraft Launcher",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:local": "bash ../../scripts/dev-backend.sh",
    "prod": "bash ../../scripts/prod-backend.sh",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "list-users": "tsx scripts/list-users.ts",
    "create-profile-1.21": "tsx scripts/create-profile-1.21.ts",
    "create-profile-1.12.2": "tsx scripts/add-profile-1.12.2.ts",
    "create-profile-1.12.2-forge": "tsx scripts/add-profile-1.12.2-forge.ts",
    "create-profile-custom": "tsx scripts/create-profile.ts",
    "list-profiles": "tsx scripts/list-profiles.ts",
    "download-minecraft": "tsx scripts/download-minecraft.ts",
    "add-client-version": "tsx scripts/add-client-version.ts",
    "create-version-1.20.4": "tsx scripts/create-version-1.20.4.ts",
    "create-version-1.12.2": "tsx scripts/create-version-1.12.2.ts",
    "apply-statistics-migration": "tsx scripts/apply-statistics-migration.ts",
    "apply-launcher-errors-migration": "tsx scripts/apply-launcher-errors-migration.ts",
    "apply-notifications-migration": "tsx scripts/apply-notifications-migration.ts",
    "apply-launcher-versions-migration": "tsx scripts/apply-launcher-versions-migration.ts",
    "apply-user-role-migration": "tsx scripts/apply-user-role-migration.ts",
    "apply-user-ban-fields-migration": "tsx scripts/apply-user-ban-fields-migration.ts",
    "apply-all-migrations": "npm run apply-user-role-migration && npm run apply-user-ban-fields-migration && npm run apply-notifications-migration && npm run apply-launcher-errors-migration && npm run apply-statistics-migration && npm run apply-launcher-versions-migration",
    "add-launcher-version": "tsx scripts/add-launcher-version.ts",
    "cli": "tsx src/cli/index.ts",
    "test": "echo \"No tests configured yet for @modern-launcher/backend\"",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,js,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,css,md}\""
  },
  "dependencies": {
    "@modern-launcher/shared": "*",
    "@prisma/client": "^6.0.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "chokidar": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "extract-zip": "^2.0.1",
    "gif-decoder": "^1.2.2",
    "gifuct-js": "^2.1.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "minecraft-server-util": "^5.4.4",
    "multer": "^2.0.2",
    "node-rsa": "^1.1.1",
    "readline": "^1.3.0",
    "recharts": "^3.4.1",
    "skinview3d": "^3.4.1",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.10.1",
    "@types/ws": "^8.18.1",
    "mysql2": "^3.15.3",
    "prisma": "^6.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
