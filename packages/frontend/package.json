{
  "name": "@modern-launcher/frontend",
  "version": "1.0.280",
  "description": "Modern Minecraft Launcher - Tauri Frontend",
  "homepage": "https://github.com/modern-launcher/modern-launcher",
  "author": {
    "name": "Modern Launcher Team",
    "email": "team@modern-launcher.com"
  },
  "scripts": {
    "dev": "vite",
    "dev:local": "bash ../../scripts/dev-frontend.sh",
    "prod": "bash ../../scripts/prod-frontend.sh",
    "build": "vite build 2>&1 | grep -v 'CJS build of Vite' || true",
    "test": "echo \"No tests configured yet for @modern-launcher/frontend\"",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "tauri:build:release": "tauri build",
    "update-launcher": "node scripts/update-launcher-full.js",
    "update-launcher:win": "node scripts/update-launcher-full.js"
  },
  "dependencies": {
    "@modern-launcher/shared": "*",
    "@tanstack/react-query": "^5.90.11",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/plugin-dialog": "^2.4.2",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "mysql2": "^3.15.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "vite": "^7.2.4"
  }
}
