# Server Configuration
PORT=7240
HOST=0.0.0.0
NODE_ENV=development

# Database
DATABASE_URL="postgresql://launcher:password@localhost:5432/launcher?schema=public"

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRY=24h

# RSA Keys (will be generated automatically if not provided)
RSA_PUBLIC_KEY_PATH=./keys/public.key
RSA_PRIVATE_KEY_PATH=./keys/private.key

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_MAX_ATTEMPTS=5
RATE_LIMIT_WINDOW_MS=60000

# Texture Provider
TEXTURE_PROVIDER=mojang
TEXTURE_CACHE_ENABLED=true

# Updates
COMPRESSION_ENABLED=true
UPDATE_CHECK_INTERVAL=300000

# Logging
LOG_LEVEL=info
